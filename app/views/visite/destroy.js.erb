<%- @cliente.appunti.each do |a|  %>
  appunto = $("#<%= dom_id(a) %>")
  appunto.toggleClass('favorited');
  $('.baule', appunto).html("<%= escape_javascript( baule_tag @cliente, class_scarica: 'btn' ) %>")
<% end %>

cliente = $("#<%= dom_id(@cliente) %>")
cliente.toggleClass('favorited');
$('.baule_btn', cliente).replaceWith("<%= escape_javascript( baule_tag @cliente, class_scarica: 'btn' ) %>")

$(".nav-baule sup").text("<%= current_user.visite.nel_baule.size %>");